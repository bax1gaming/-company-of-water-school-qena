{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "framework": "vite",
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        "cache-control": "max-age=31536000, immutable"
      ]
    }
  ]
}